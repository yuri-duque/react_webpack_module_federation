{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "baseUrl": "./",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "sourceMap": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "uiElements/ContainedButton": [
        "./ui-elements/src/Button/ContainedButton"
      ],
      "uiElements/Card": ["./ui-elements/src/Card/Card"]
    }
  },
  "include": ["*/src"]
}
